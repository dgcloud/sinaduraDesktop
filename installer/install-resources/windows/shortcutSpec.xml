<shortcuts>

<programGroup defaultName="Sinadura" location="applications||startMenu" />

	<shortcut name="Sinadura - Firma Digital"
		programGroup="yes"
		desktop="yes"
		applications="no"
		startMenu="no"
		startup="no"
		target="$INSTALL_PATH\sinadura.exe"
		iconFile="$INSTALL_PATH\resources\images\sinadura.ico"
		workingDirectory="$INSTALL_PATH"
		commandLine=""
		terminal="false"
		description="Starts Sinadura - Firma Digital"
		initialState="noShow">
		<createForPack name="sinadura" />
	</shortcut>

	<shortcut name="Uninstall Sinadura - Firma Digital"
		programGroup="yes"
		desktop="no"
		applications="no"
		startMenu="no" startup="no"
		target="java.exe"
		workingDirectory="$INSTALL_PATH\Uninstaller\"
		commandLine="-jar uninstaller.jar"
		iconFile="%SystemRoot%\system32\SHELL32.dll"
		iconIndex="31"
		description="Uninstall Sinadura - Firma Digital">
		<createForPack name="sinadura" />
	</shortcut>

</shortcuts>


